{
  "name": "unanimous-game",
  "private": true,
  "type": "module",
  "scripts": {
  "dev": "wrangler dev --ip 0.0.0.0",
  "dev:local": "wrangler dev",
  "dev:lan": "wrangler dev --ip 0.0.0.0",
    "dev:client": "npm --prefix src/client run dev",
    "build:client": "npm --prefix src/client ci && npm --prefix src/client run build",
    "build:production": "npm --prefix src/client install && npm --prefix src/client run build",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "hono": "^4.6.10"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250807.0",
    "wrangler": "^4.28.1"
  }
}
